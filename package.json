{
  "name": "rn-expo-stack",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "android": "expo start --android",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier:fix": "prettier '**/*.{js,jsx,ts,tsx,json,md}' --write",
    "ios": "expo start --ios",
    "start": "expo start",
    "web": "expo start --web",
    "analyze:bundle": "react-native-bundle-visualizer --only-mapped --entry-file ./index.js --bundle-output ./rn.bundle --expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "expo": "~50.0.7",
    "expo-constants": "~15.4.5",
    "expo-linking": "~6.2.2",
    "expo-localization": "~14.8.3",
    "expo-router": "^3.4.7",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "i18next": "^23.7.20",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.0.1",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@types/node": "^20",
    "@types/react": "^18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react-native-bundle-visualizer": "^3.1.3",
    "typescript": "5.3.3"
  },
  "resolutions": {
    "expo-modules-core": "~1.11.0"
  },
  "overrides": {
    "expo-modules-core": "~1.11.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --fix",
    "*.{js,css,md,ts,tsx,jsx}": "prettier --write"
  },
  "private": true
}
